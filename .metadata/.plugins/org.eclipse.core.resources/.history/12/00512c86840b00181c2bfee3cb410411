object higherOrderFcs {

val double = (i: Int) => i*2                      //> double  : Int => Int = higherOrderFcs$$$Lambda$3/1854731462@4b85612c

def higherOrder(x: Int, y: Int=>Int) = y(x)       //> higherOrder: (x: Int, y: Int => Int)Int


higherOrder(6, double)                            //> res0: Int = 12

}