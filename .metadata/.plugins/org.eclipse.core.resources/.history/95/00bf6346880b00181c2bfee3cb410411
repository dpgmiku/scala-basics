object payrollChallenge {
var rate = 10.50
def earnings(s: String, h: Double): String = {
if (s != "y"){
if (h <= 40)
"Salary is: $" + h*rate


else {"Salary is: $ " + (40*rate+(h-40)*rate*1.5)}
}
else "This is a salaried employee"}

println(earnings("n", 40))
println(earnings("n", 45))
println(earnings("n", 25))
println(earnings("y", 0))


}